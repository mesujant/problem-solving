{
  "manifest_version": 2,
  "name": "spell check",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [ "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
